@model Guid
<div class="modal-dialog modal-dialog-centered">
    <div class="modal-content bg-dark">
        <div class="modal-header">
        <h5 class="modal-title text-white" id="staticBackdropLabel">Вы точно хотите удальть альбом</h5>
        <button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
        <form>
            <div class="d-flex justify-content-end mb-1">
                <button onclick="CloseModelWindow()" type="button" class="btn btn-outline-secondary">Нет</button>
                <a asp-action="DeleteImageAlbum" asp-controller="ImageAlbum" asp-route-imageAlbumId="@Model" type="button" class="btn btn-outline-secondary ms-3">Да, удалить</a>
            </div>
        </form>
        </div>
    </div>
</div>